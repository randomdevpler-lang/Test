-- Scripts:

local function OLIFAIY_fake_script() -- LocationTp.Main 
	local script = Instance.new('LocalScript', LocationTp)

	-- SERVICES
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local UIS = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	local enabled = false
	-- GUI (LocalScript is inside Frame)
	local frame = script.Parent
	local saveBtn = frame:WaitForChild("SavePos")
	local goBtn = frame:WaitForChild("GoPos")
	local label = frame:WaitForChild("Title")
	local enabled = false
	
	local function getChar()
		return player.Character
	end
	
	local function getHum()
		local char = getChar()
		return char and char:FindFirstChildOfClass("Humanoid")
	end
	
	-- SETTINGS
	local TOOL_NAME = "Flying Carpet" or "Witch's Broom"
	local savedCFrame
	
	-- Get character safely
	local function getChar()
		return player.Character or player.CharacterAdded:Wait()
	end
	
	
	-- Equip + activate Flying Carpet
	local function equipAndActivate()
		local char = getChar()
		local backpack = player:WaitForChild("Backpack")
	
		local tool = backpack:FindFirstChild(TOOL_NAME) or char:FindFirstChild(TOOL_NAME)
		if not tool then
			label.Text = "Tool not found!"
			task.wait(3)
			label.Text = "TP LOCATION"
			return false
		end
	
		tool.Parent = char
		task.wait(0.2)
	
		pcall(function()
			tool:Activate()
		end)
	
		return true
	end
	
	local function enableGodMode()
		local hum = getHum()
		if not hum then return end
	
		hum.BreakJointsOnDeath = false
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
		hum.MaxHealth = math.huge
		hum.Health = hum.MaxHealth
	
		healthConn = hum.HealthChanged:Connect(function()
			if enabled then
				hum.Health = hum.MaxHealth
			end
		end)
	end
	
	local function disableGodMode()
		local hum = getHum()
		if not hum then return end
	
		if healthConn then
			healthConn:Disconnect()
			healthConn = nil
		end
	
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
		hum.BreakJointsOnDeath = true
		hum.MaxHealth = 100
		hum.Health = 100
	end
	
	-- SAVE POSITION
	saveBtn.MouseButton1Click:Connect(function()
		local char = getChar()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if not hrp then return end
	
		savedCFrame = hrp.CFrame
		label.Text = "Position Saved"
	end)
	
	-- GO TO POSITION (SLOW STEP TELEPORT)
	goBtn.MouseButton1Click:Connect(function()
		if not savedCFrame then
			label.Text = "No position saved"
			return
		end
	
		-- ENABLE GOD MODE BEFORE TP
		pcall(function()
			enableGodMode()
		end)
	
		if not equipAndActivate() then 
			disableGodMode()
			return 
		end
	
		task.wait(0.3)
	
		local char = getChar()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if not hrp then 
			disableGodMode()
			return 
		end
	
		label.Text = "Teleporting..."
	
		local targetPos = savedCFrame.Position
		local stepSize = 5
		local stepDelay = 0.05
	
		while (hrp.Position - targetPos).Magnitude > stepSize do
			local direction = (targetPos - hrp.Position).Unit
			local nextPos = hrp.Position + direction * stepSize
	
			hrp.AssemblyLinearVelocity = Vector3.zero
			hrp.CFrame = CFrame.new(nextPos, nextPos + hrp.CFrame.LookVector)
	
			task.wait(stepDelay)
		end
	
		-- Final exact position + rotation
		hrp.AssemblyLinearVelocity = Vector3.zero
		hrp.CFrame = savedCFrame
	
		label.Text = "Arrived"
	
		-- DISABLE GOD MODE AFTER TP
		pcall(function()
			disableGodMode()
		end)
	
		task.wait(3)
		label.Text = "TP LOCATION"
	end)
	
	
end
coroutine.wrap(OLIFAIY_fake_script)()
local function YOJQBZJ_fake_script() -- iNFjUMP.LocalScript 
	local script = Instance.new('LocalScript', iNFjUMP)

	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local enabled = false
	local jumpConn, healthConn, posConn
	local lastCFrame
	
	button.Text = "OFF"
	
	local function getChar()
		return player.Character
	end
	
	local function getHum()
		local char = getChar()
		return char and char:FindFirstChildOfClass("Humanoid")
	end
	
	local function getRoot()
		local char = getChar()
		return char and char:FindFirstChild("HumanoidRootPart")
	end
	
	-- GODMODE
	local function enableGodMode()
		local hum = getHum()
		if not hum then return end
	
		hum.BreakJointsOnDeath = false
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
		hum.MaxHealth = math.huge
		hum.Health = hum.MaxHealth
	
		healthConn = hum.HealthChanged:Connect(function()
			if enabled then
				hum.Health = hum.MaxHealth
			end
		end)
	end
	
	local function disableGodMode()
		local hum = getHum()
		if not hum then return end
	
		if healthConn then
			healthConn:Disconnect()
			healthConn = nil
		end
	
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
		hum.BreakJointsOnDeath = true
		hum.MaxHealth = 100
		hum.Health = 100
	end
	
	-- INFINITE JUMP
	local function enableInfiniteJump()
		jumpConn = UIS.JumpRequest:Connect(function()
			if enabled then
				local hum = getHum()
				if hum then
					hum:ChangeState(Enum.HumanoidStateType.Jumping)
				end
			end
		end)
	end
	
	local function disableInfiniteJump()
		if jumpConn then
			jumpConn:Disconnect()
			jumpConn = nil
		end
	end
	
	-- ANTI TELEPORT
	local function enableAntiTP()
		posConn = RunService.Heartbeat:Connect(function()
			if not enabled then return end
			local root = getRoot()
			if not root then return end
	
			if not lastCFrame then
				lastCFrame = root.CFrame
			end
	
			-- If something tries to teleport you
			if (root.CFrame.Position - lastCFrame.Position).Magnitude > 10 then
				root.CFrame = lastCFrame
			else
				lastCFrame = root.CFrame
			end
		end)
	end
	
	local function disableAntiTP()
		if posConn then
			posConn:Disconnect()
			posConn = nil
		end
		lastCFrame = nil
	end
	
	-- BUTTON TOGGLE
	button.MouseButton1Click:Connect(function()
		enabled = not enabled
	
		if enabled then
			button.Text = "ON"
			enableInfiniteJump()
			enableGodMode()
			enableAntiTP()
		else
			button.Text = "OFF"
			disableInfiniteJump()
			disableGodMode()
			disableAntiTP()
		end
	end)
	
	-- RESPAWN SAFETY
	player.CharacterAdded:Connect(function()
		if enabled then
			task.wait(0.2)
			enableGodMode()
			enableAntiTP()
		end
	end)
	
end
coroutine.wrap(YOJQBZJ_fake_script)()
local function VJZHG_fake_script() -- Teleport.LocalScript 
	local script = Instance.new('LocalScript', Teleport)

	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local EQUIP_TIME = 0.2 -- seconds
	
	button.MouseButton1Click:Connect(function()
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")
		local backpack = player:WaitForChild("Backpack")
	
		local tool = backpack:FindFirstChild("Flying Carpet") or backpack:FindFirstChild("Witch's Broom")
		if not tool then return end
	
		-- Equip ONCE
		humanoid:EquipTool(tool)
	
		local startTime = os.clock()
		local activated = false
	
		local connection
		connection = RunService.Heartbeat:Connect(function()
			if os.clock() - startTime >= EQUIP_TIME then
				connection:Disconnect()
				humanoid:UnequipTools()
				return
			end
	
			-- Re-equip ONLY if Roblox forces it off
			if tool.Parent ~= character then
				humanoid:EquipTool(tool)
			end
	
			-- Activate once, only when tool is ready
			if not activated and tool.Enabled then
				activated = true
				tool:Activate()
			end
		end)
	end)
	
	
	button.MouseButton1Click:Connect(function()
		enabled = not enabled
	
		if enabled then
			enableInfiniteJump()
			enableGodMode()
			enableAntiTP()
		else
			disableInfiniteJump()
			disableGodMode()
			disableAntiTP()
		end
	end)
	
end
coroutine.wrap(VJZHG_fake_script)()
local function BVYO_fake_script() -- Teleport.Tpforward 
	local script = Instance.new('LocalScript', Teleport)

	local Players = game:GetService("Players")
	local TweenService = game:GetService("TweenService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local character = player.Character or player.CharacterAdded:Wait()
		local root = character:WaitForChild("HumanoidRootPart")
		local humanoid = character:WaitForChild("Humanoid")
	
		-- Settings
		local DISTANCE = 5
		local BOOST_SPEED = 80
		local NORMAL_SPEED = 16
	
		-- Set speed (visual / animation consistency)
		humanoid.WalkSpeed = BOOST_SPEED
	
		-- Calculate target position
		local targetCFrame = root.CFrame + (root.CFrame.LookVector * DISTANCE)
	
		-- Tween (smooth forward movement)
		local tween = TweenService:Create(
			root,
			TweenInfo.new(
				DISTANCE / BOOST_SPEED, -- duration based on speed
				Enum.EasingStyle.Linear,
				Enum.EasingDirection.Out
			),
			{ CFrame = targetCFrame }
		)
	
		tween:Play()
	
		tween.Completed:Once(function()
			humanoid.WalkSpeed = NORMAL_SPEED
		end)
	end)
	
end
coroutine.wrap(BVYO_fake_script)()
local function WWTQU_fake_script() -- Teleport.Health 
	local script = Instance.new('LocalScript', Teleport)

	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	local enabled = false
	
	local function getChar()
		return player.Character
	end
	
	local function getHum()
		local char = getChar()
		return char and char:FindFirstChildOfClass("Humanoid")
	end
	
	
	local function enableGodMode()
		local hum = getHum()
		if not hum then return end
	
		hum.BreakJointsOnDeath = false
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
		hum.MaxHealth = math.huge
		hum.Health = hum.MaxHealth
	
		healthConn = hum.HealthChanged:Connect(function()
			if enabled then
				hum.Health = hum.MaxHealth
			end
		end)
	end
	
	local function disableGodMode()
		local hum = getHum()
		if not hum then return end
	
		if healthConn then
			healthConn:Disconnect()
			healthConn = nil
		end
	
		hum:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
		hum.BreakJointsOnDeath = true
		hum.MaxHealth = 100
		hum.Health = 100
	end
	
	
	
	button.MouseButton1Click:Connect(function()
		enableGodMode() 
		wait(0.1)
		disableGodMode() 
	end)
	
	
end
coroutine.wrap(WWTQU_fake_script)()
local function LNIPZO_fake_script() -- Teleport_2.LocalScript 
	local script = Instance.new('LocalScript', Teleport_2)

	-- SERVICES
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	-- =====================
	-- SETTINGS (CHANGE HERE)
	-- =====================
	local SPEED = 6       -- ðŸ”¥ CHANGE THIS FOR SPEED (0.6â€“1.2 recommended)
	
	-- =====================
	-- STATE
	-- =====================
	local enabled = false
	local moveLoop
	local godLoop
	
	local keys = {
		W = false,
		A = false,
		S = false,
		D = false
	}
	
	-- =====================
	-- CHARACTER GETTERS
	-- =====================
	local function getChar()
		return player.Character or player.CharacterAdded:Wait()
	end
	
	local function getHumanoid()
		return getChar():WaitForChild("Humanoid")
	end
	
	local function getRoot()
		return getChar():WaitForChild("HumanoidRootPart")
	end
	
	-- =====================
	-- INPUT TRACKING
	-- =====================
	UserInputService.InputBegan:Connect(function(input, gpe)
		if gpe then return end
		if keys[input.KeyCode.Name] ~= nil then
			keys[input.KeyCode.Name] = true
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if keys[input.KeyCode.Name] ~= nil then
			keys[input.KeyCode.Name] = false
		end
	end)
	
	-- =====================
	-- CFRAME WALK
	-- =====================
	local function startCFrameWalk()
		if moveLoop then moveLoop:Disconnect() end
	
		moveLoop = RunService.RenderStepped:Connect(function()
			if not enabled then return end
	
			local root = getRoot()
			local cam = workspace.CurrentCamera
			local move = Vector3.zero
	
			if keys.W then move += cam.CFrame.LookVector end
			if keys.S then move -= cam.CFrame.LookVector end
			if keys.A then move -= cam.CFrame.RightVector end
			if keys.D then move += cam.CFrame.RightVector end
	
			if move.Magnitude > 0 then
				root.CFrame = root.CFrame + (move.Unit * SPEED)
			end
		end)
	end
	
	local function stopCFrameWalk()
		if moveLoop then
			moveLoop:Disconnect()
			moveLoop = nil
		end
	end
	
	-- =====================
	-- GODMODE + ANTI RAGDOLL
	-- =====================
	local function startGodmode()
		if godLoop then godLoop:Disconnect() end
	
		godLoop = RunService.Stepped:Connect(function()
			local humanoid = getHumanoid()
			local root = getRoot()
	
			-- INFINITE HEALTH
			humanoid.MaxHealth = math.huge
			humanoid.Health = math.huge
	
			-- BLOCK DEATH
			humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
	
			-- ANTI RAGDOLL / STUN
			humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
			humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
			humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, false)
			humanoid:ChangeState(Enum.HumanoidStateType.Running)
	
			-- ANTI KNOCKBACK / FLING
			root.AssemblyLinearVelocity = Vector3.zero
			root.AssemblyAngularVelocity = Vector3.zero
		end)
	end
	
	local function stopGodmode()
		if godLoop then
			godLoop:Disconnect()
			godLoop = nil
		end
	end
	
	-- =====================
	-- TOGGLE BUTTON
	-- =====================
	button.MouseButton1Click:Connect(function()
		enabled = not enabled
	
		if enabled then
			button.Text = "ON"
			startCFrameWalk()
			startGodmode()
		else
			button.Text = "OFF"
			stopCFrameWalk()
			stopGodmode()
		end
	end)
	
	-- =====================
	-- RESPAWN SAFE
	-- =====================
	player.CharacterAdded:Connect(function()
		if enabled then
			task.wait(0.2)
			startCFrameWalk()
			startGodmode()
		end
	end)
	
end
coroutine.wrap(LNIPZO_fake_script)()
local function CJJVCJC_fake_script() -- PanicButton.LocalScript 
	local script = Instance.new('LocalScript', PanicButton)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		-- Find the ScreenGui this button belongs to
		local gui = button:FindFirstAncestorOfClass("ScreenGui")
	
		if gui then
			gui:Destroy()
		end
	end)
	
end
coroutine.wrap(CJJVCJC_fake_script)()
local function GFXETEQ_fake_script() -- MainFrame.Drag 
	local script = Instance.new('LocalScript', MainFrame)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
	
end
coroutine.wrap(GFXETEQ_fake_script)()
